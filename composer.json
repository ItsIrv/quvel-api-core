{
    "name": "quvel/core",
    "description": "Core foundation package for Quvel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irving Gomez <gomirv.work@gmail.com>"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/log": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Quvel\\Core\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Quvel\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quvel\\Core\\Providers\\CoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}